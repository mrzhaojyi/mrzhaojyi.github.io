import openai
openai.api_key = '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'
ask_text = "what is a concealed tip in Chinese calligraphy"

response = openai.Completion.create(
    engine = "text-davinci-003",    # select model
    prompt = ask_text,     
    max_tokens = 512,               # response tokens
    temperature = 1,                # diversity related
    top_p = 0.75,                   # diversity related
    n = 1,                          # num of response
)

completed_text = response["choices"][0]["text"]
print(completed_text)
